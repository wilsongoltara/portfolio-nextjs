{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"plugin:react/recommended",
		"airbnb",
		"plugin:@typescript-eslint/recommended",
		"plugin:prettier/recommended",
		"eslint:recommended",
		"prettier"
	],
	"overrides": [
	],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": [
		"react",
		"react-hooks",
		"@typescript-eslint", 
  	"prettier"
	],
	"rules": {
		"import/no-unresolved": "error"
	},
	"settings": {
		"import/resolver": {
			"typescript": {}
		}
	}
}
